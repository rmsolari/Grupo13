<h1>Añadir nuevo género</h1>
<div clas="container">
  <div class="jumbotron">
    <%= form_for(@gender) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :nombre %>
    <%= f.text_area :name, placeholder: "Ingresa nombre de género.." %>
    <%= f.submit "Añadir", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
